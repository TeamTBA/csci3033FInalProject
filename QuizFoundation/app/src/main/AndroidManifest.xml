<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teamtba.quizfoundation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launch_icon_new"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/launch_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".QuizResults" />
        <activity
            android:name=".QuestionEditorActivity"
            android:label="@string/title_activity_question_editor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".QuizSelector">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuizAction"></activity>
    </application>

</manifest>